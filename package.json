    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
